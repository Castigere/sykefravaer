{
  "name": "digisyfo",
  "version": "2.0.0",
  "description": "Digitalisering av sykefraværsoppfølging",
  "main": "index.js",
  "test": "npm run test",
  "scripts": {
    "postinstall": "npm install babel-loader",
    "dev": "title syfofront && parallelshell \"npm run images\" \"npm run static:build:local\" \"npm run scripts:watch\"",
    "build": "parallelshell \"npm run deploy\" \"npm run images\"",
    "test:watch": "title syfofront-test && cross-env NODE_ENV=test nyc mocha --watch -G --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
    "test": "cross-env NODE_ENV=test nyc mocha -G --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
    "posttest": "nyc report --reporter=html --reporter=lcov",
    "static:build:local": "node build-html.js local",
    "images": "npm run images:copy:nav-frontend && npm run images:install",
    "images:copy:nav-frontend": "cp -R node_modules/nav-frontend-grafikk img",
    "images:install": "cp -R img ../main/webapp",
    "deploy": "node deploy.js",
    "scripts:watch": "webpack-dev-server --inline --port 9090",
    "lint": "eslint js/. --ext .js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "DigiSyfo <digisyfo@nav.no>",
  "license": "ISC",
