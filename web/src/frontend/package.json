{
  "name": "digisyfo",
  "version": "3.0.0",
  "description": "Digitalisering av sykefraværsoppfølging",
  "main": "index.js",
  "test": "npm run test",
  "scripts": {
    "analyze": "source-map-explorer ../main/webapp/js/bundle-prod.*",
    "postinstall": "npm install babel-loader",
    "dev": "npm-run-all images static:build:local scripts:watch filmtekster",
    "build": "npm-run-all deploy images filmtekster",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill ./test/setup.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch",
    "coverage": "nyc report --reporter=html --reporter=lcov",
    "static:build:local": "node build-html.js local",
    "filmtekster": "cp -R filmtekster ../main/webapp",
    "images": "npm run images:copy:nav-frontend && npm run images:install",
    "images:copy:nav-frontend": "cp -R node_modules/nav-frontend-grafikk img",
    "images:install": "cp -R img ../main/webapp",
    "deploy": "node deploy.js",
    "scripts:watch": "webpack-dev-server --inline --port 9090",
    "lint": "npm-run-all lintJs lintTest",
    "lint:fix": "npm-run-all lintJs:fix lintTest:fix",
    "lintJs": "eslint js/. --ext .js",
    "lintJs:fix": "eslint js/. --ext .js --fix",
    "lintTest": "eslint test/. --ext .js",
    "lintTest:fix": "eslint test/. --ext .js --fix"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "DigiSyfo <digisyfo@nav.no>",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.5",
    "digisyfo-npm": "9.0.0",
    "fetch": "1.0.1",
    "isomorphic-fetch": "2.2.1",
    "moter-npm": "5.0.2",
    "nav-frontend-brodsmuler-design": "1.2.1",
    "nav-frontend-core": "2.0.1",
    "nav-frontend-grafikk": "1.0.7",
    "nav-frontend-hjelpetekst-design": "1.3.1",
    "nav-frontend-knapper-design": "2.0.4",
    "nav-frontend-skjema-design": "2.0.0",
    "nav-frontend-stegindikator-design": "0.1.2",
    "nav-frontend-typografi-design": "2.1.1",
    "nav-logger": "2.0.3",
