{
  "name": "digisyfo",
  "version": "0.0.1",
  "description": "Digitalisering av sykefraværsoppfølging",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "parallelshell \"npm run static:build:local\" \"npm run images:copy\" \"npm run scripts:watch\" \"npm run styles:watch\"",
    "build": "parallelshell \"npm run static:build\" \"npm run scripts:build\" \"npm run vendor:build\" \"npm run styles:build\"",
    "postinstall": "npm run build",
    "test": "mocha --compilers js:babel-core/register --require ./tests/test_helper.js \"tests/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch",
    "styles:watch": "nodemon -q -w styles --ext \"less\" --exec \"npm run styles:build\"",
    "styles:build": "lessc styles/styles.less > ../main/webapp/styles/styles.css --autoprefix=\"last 3 versions\"",
    "static:build:local": "mustache settingsLocal.json html/syfofront.mustache > ../main/webapp/syfofront.html",
    "static:build": "parallelshell \"mustache settings.json html/syfofront.mustache > ../main/webapp/syfofront.html\" \"mustache settings.json html/syfofeil.mustache > ../main/webapp/syfofeil.html\"",
    "images:copy": "cp -R node_modules/modig-frontend/modig-frontend-ressurser/src/main/resources/META-INF/resources/img/ikoner-svg public/img",
    "vendor:build": "cp -R vendor ../main/webapp",
    "scripts:build": "set NODE_ENV=production&&node deploy.js",
    "scripts:watch": "webpack-dev-server --inline",
    "lint": "eslint js/. --ext .js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Helge Fredheim <helge.fredheim@nav.no>",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "cheerio": "^0.20.0",
    "deep-freeze": "0.0.1",
    "eslint": "^2.5.3",
    "express": "^4.13.4",
    "fetch": "^1.0.1",
    "http-proxy": "^1.13.2",
    "immutable": "^3.7.6",
    "jquery": "^2.2.1",
    "moment": "^2.11.2",
    "path": "^0.12.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "react-scroll": "^1.0.3",
    "redux": "^3.3.1",
    "redux-form": "^4.2.0",
    "request": "^2.69.0",
    "scroll-behavior": "^0.3.2",
    "webpack": "^1.12.13",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-eslint": "^6.0.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "express-livereload": "0.0.24",
    "fs": "0.0.2",
    "jsdom": "^8.0.4",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "less-plugin-autoprefix": "^1.5.1",
    "mocha": "^2.4.5",
    "modig-frontend": "git+http://stash.devillo.no/scm/felles/modig-frontend.git#v3.0.96",
    "mustache": "^2.2.1",
    "nodemon": "^1.8.1",
    "parallelshell": "^2.0.0",
    "proxy-middleware": "^0.15.0",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "run-sequence": "^1.1.5",
    "style-loader": "^0.13.0",
    "svg-loader": "0.0.2",
    "vinyl-source-stream": "^1.1.0",
    "webpack-dev-server": "^1.14.1"
  }
}
