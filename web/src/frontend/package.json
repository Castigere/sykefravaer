{
  "name": "digisyfo",
  "version": "0.0.1",
  "description": "Digitalisering av sykefraværsoppfølging",
  "main": "index.js",
  "test": "npm run test",
  "scripts": {
    "prebuild": "node createProdFolders.js",
    "dev": "parallelshell \"npm run prebuild\" \"npm run images\" \"npm run vendor:build\" \"npm run static:build:local\" \"npm run scripts:watch\" \"npm run styles:watch\"",
    "build": "parallelshell \"npm run prebuild\" \"npm run static:build\" \"npm run scripts:build\" \"npm run images\" \"npm run vendor:build\" \"npm run styles:build\"",
    "test:watch": "cross-env NODE_ENV=test nyc mocha --watch -G --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
    "test": "cross-env NODE_ENV=test nyc mocha -G --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
    "posttest": "nyc report --reporter=html --reporter=lcov",
    "styles:watch": "nodemon -q -w styles --ext \"less\" --exec \"npm run styles:build\"",
    "styles:build": "lessc styles/styles.less > ../main/webapp/styles/styles.css --autoprefix=\"last 3 versions\"",
    "static:build:local": "node build-html.js html/syfofront.mustache local",
    "static:build": "node build-html.js html/syfofront.mustache prod",
    "images": "parallelshell \"npm run images:copy\" \"npm run images:install\"",
    "images:copy": "cp -R node_modules/modig-frontend/modig-frontend-ressurser/src/main/resources/META-INF/resources/img img/modig-frontend",
    "images:install": "cp -R img ../main/webapp",
    "vendor:build": "cp -R vendor ../main/webapp",
    "scripts:build": "set NODE_ENV=production&&node deploy.js",
    "scripts:watch": "webpack-dev-server --inline",
    "lint": "eslint js/. --ext .js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "DigiSyfo <digisyfo@nav.no>",
  "license": "ISC",
  "dependencies": {
    "babel": "6.3.26",
    "babel-cli": "6.7.7",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.3",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.3.13",
    "babelify": "7.2.0",
    "cheerio": "0.20.0",
    "chokidar": "1.6.0",
    "digisyfo-npm": "1.0.13",
    "fetch": "1.0.1",
    "http-proxy": "1.13.2",
    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.1",
    "jquery": "2.2.1",
    "jsfmt": "0.5.3",
    "path": "0.12.7",
    "promise-polyfill": "6.0.2",
    "react": "15.3.2",
    "react-document-title": "2.0.1",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.5",
    "react-scroll": "1.2.0",
    "redux": "3.5.2",
    "redux-form": "6.0.5",
    "redux-saga": "0.12.0",
    "request": "2.69.0",
    "scroll-behavior": "0.3.2",
    "simple-ajax": "2.6.0",
    "webpack": "1.12.13",
    "whatwg-fetch": "0.11.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.5.2",
    "babel-eslint": "6.0.0",
    "babel-istanbul": "0.11.0",
    "babel-plugin-istanbul": "2.0.3",
    "babel-preset-es2015-without-strict": "0.0.3",
    "chai": "3.5.0",
    "chai-enzyme": "0.4.2",
    "chai-immutable": "1.5.3",
    "chai-jquery": "2.0.0",
    "cross-env": "3.1.3",
    "css-loader": "0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "2.2.0",
    "eslint": "2.5.3",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.2.3",
    "fetch-mock": "5.5.0",
    "fs": "0.0.2",
    "jsdom": "8.0.4",
    "jsfmt": "0.5.3",
    "less": "2.6.0",
    "less-loader": "2.2.2",
    "less-plugin-autoprefix": "1.5.1",
    "mocha": "3.1.2",
    "modig-frontend": "git+http://stash.devillo.no/scm/felles/modig-frontend.git#modig-frontend-4.0.10",
    "mustache": "2.2.1",
    "nodemon": "1.8.1",
    "nyc": "8.3.2",
    "parallelshell": "2.0.0",
    "proxy-middleware": "0.15.0",
    "react-addons-test-utils": "0.14.7",
    "react-hot-loader": "1.3.0",
    "redux-mock-store": "1.2.1",
    "run-sequence": "1.1.5",
    "sinon": "1.17.3",
    "style-loader": "0.13.0",
    "svg-loader": "0.0.2",
    "vinyl-source-stream": "1.1.0",
    "webpack-dev-server": "1.16.2"
  }
}
